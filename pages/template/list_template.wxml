<template name="listTmp">
<view class='tmpContainer'>
  <view class='avatar_date'>
    <image src='{{avatar}}'></image>
    <text>{{date}}</text>
  </view>
  <text class='company'>{{title}}</text>
  <image class='contentImg' src='{{detail_img}}'></image>
  <text class='content'>
    {{detail_content}}
  </text>
  <view class='collection_love'>
    <image src='/images/icon/star.png'></image><text>{{love_count}}</text>
    <image src='/images/icon/eye.png'></image><text>{{attention_count}}</text>
  </view>
</view>
</template>